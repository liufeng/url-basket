<h1>Temporary URLs</h1>

<table border="1" width=800>
  <tr>
    <th>Url</th>
    <th>Title</th>
    <th></th>
  </tr>

<% @urls.each do |url| %>
  <tr>
    <td><%= url.url %></td>
    <td><%= url.title %></td>
    <td>
      <%= link_to 'Show', url %> 
      <%= link_to 'Edit', edit_url_path(url) %> 
      <%= link_to 'Destroy', url, confirm: 'Are you sure?', method: :delete %>
    </td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Url', new_url_path %>
